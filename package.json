{
  "name": "banner-settings-improved-app",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build:dev": "npm run compile:dev",
    "package:dev": "npm run package:vsts-extension --vssconfig=dev",
    "publish:dev": "npm run publish:vsts-extension --vssconfig=dev",
    "build:test": "npm run compile",
    "package:test": "npm run package:vsts-extension --vssconfig=test",
    "publish:test": "npm run publish:vsts-extension --vssconfig=test",
    "build:prod": "npm run compile",
    "package:prod": "npm run package:vsts-extension --vssconfig=prod",
    "publish:prod": "npm run publish:vsts-extension --vssconfig=prod",
    "package:vsts-extension": "tfx extension create --manifests ./ado-manifests/azure-devops-extension-base.json --overrides-file ./ado-manifests/azure-devops-extension-$npm_config_vssconfig.json",
    "publish:vsts-extension": "tfx extension publish --manifests ./ado-manifests/azure-devops-extension-base.json --overrides-file ./ado-manifests/azure-devops-extension-$npm_config_vssconfig.json",
    "dev": "webpack-dev-server --progress --color",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "azure-devops-extension-api": "~2.225.0",
    "azure-devops-extension-sdk": "~3.1.2",
    "azure-devops-ui": "~2.167.82",
    "moment": "~2.29.4",
    "react": "~16.14.0",
    "react-dom": "~16.14.0",
    "text-mask-core": "~5.1.2"
  },
  "devDependencies": {
    "@types/react": "~16.14.48",
    "@types/react-dom": "~16.9.20",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.17.0",
    "base64-inline-loader": "^2.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "~6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "~6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "~9.0.0",
    "path": "^0.12.7",
    "rimraf": "~5.0.5",
    "sass-loader": "~13.3.2",
    "style-loader": "~3.3.3",
    "tfx-cli": "~0.16.0",
    "ts-loader": "~9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
